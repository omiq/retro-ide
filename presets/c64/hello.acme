* = $0801
!word Start
!byte $00,$00,$9e
!text "2066"
!byte $00,$00,$00
* = $0812

Start:
  jsr $e544	; clear screen
  ldy #0
Loop:
  lda Message,y	; load message byte
  beq EOM	; 0 = end of string
  sta $400+41,y	; store to screen
  iny
  bne Loop	; next character
EOM:
  jmp EOM	; infinite loop

Message:
!scr "hello world!", 0

